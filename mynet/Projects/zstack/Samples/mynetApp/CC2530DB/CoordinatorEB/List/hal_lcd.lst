    712           *
    713           * @return      none
    714           **************************************************************************************************/
    715          void HalLcd_HW_WriteLine(uint8 line, const char *pText)
    716          {
    717            uint8 count;
    718            uint8 totalLength = (uint8)osal_strlen( (char *)pText );
    719          
