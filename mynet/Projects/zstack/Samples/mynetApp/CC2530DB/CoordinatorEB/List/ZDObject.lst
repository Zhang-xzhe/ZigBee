    578           * @return      true if a match is found
    579           */
    580          byte ZDO_AnyClusterMatches( byte ACnt, uint16 *AList, byte BCnt, uint16 *BList )
    581          {
    582            byte x, y;
    583          
    584            for ( x = 0; x < ACnt; x++ )
    585            {
    586              for ( y = 0; y < BCnt; y++ )
    587              {
    588                if ( AList[x] == BList[y] )
    589                {
    590                  return true;
    591                }
    592              }
    593            }
    594          
    595            return false;
    596          }
