    325          /*********************************************************************
    326           * @fn      _itoa
    327           *
    328           * @brief   convert a 16bit number to ASCII
    329           *
    330           * @param   num -
    331           *          buf -
    332           *          radix -
    333           *
    334           * @return  void
    335           *
    336           *********************************************************************/
    337          void _itoa(uint16 num, uint8 *buf, uint8 radix)
    338          {
    339            char c,i;
    340            uint8 *p, rst[5];
    341          
    342            p = rst;
    343            for ( i=0; i<5; i++,p++ )
    344            {
    345              c = num % radix;  // Isolate a digit
    346              *p = c + (( c < 10 ) ? '0' : '7');  // Convert to Ascii
    347              num /= radix;
    348              if ( !num )
    349                break;
    350            }
    351          
    352            for ( c=0 ; c<=i; c++ )
    353              *buf++ = *p--;  // Reverse character order
    354          
    355            *buf = '\0';
    356          }
