     96          
     97            /* parse header */
     98            cmdId = pBuf[MT_RPC_POS_CMD1];
     99            pBuf += MT_RPC_FRAME_HDR_SZ;
    100          
    101            /* Populate info */
    102            debugCompId = *pBuf++;
